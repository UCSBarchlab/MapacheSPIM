#!/usr/bin/env python3
"""
MapacheSail Console Entry Point

Launch the interactive RISC-V simulator console.
"""

import sys
from pathlib import Path

# Add parent directory to path so we can import mapachesail
sys.path.insert(0, str(Path(__file__).parent))

from mapachesail.console import main

if __name__ == '__main__':
    main()
